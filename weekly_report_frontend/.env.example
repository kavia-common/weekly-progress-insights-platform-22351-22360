# Example environment variables for Weekly Report Frontend
# Copy this file to .env and fill in values as needed.

# Supabase
REACT_APP_SUPABASE_URL=
REACT_APP_SUPABASE_KEY=

# Backend endpoints (optional placeholders for future integration)
REACT_APP_API_BASE=
REACT_APP_BACKEND_URL=
REACT_APP_WS_URL=

# App config
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_NODE_ENV=development
REACT_APP_PORT=3000
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/healthz

# Feature flags (JSON string), e.g.: {"enableInsights": true, "betaNewForm": false}
REACT_APP_FEATURE_FLAGS=

# Experiments toggle
REACT_APP_EXPERIMENTS_ENABLED=false

# Build optimizations
REACT_APP_ENABLE_SOURCE_MAPS=true

# Trust proxy (if behind proxy in deployment)
REACT_APP_TRUST_PROXY=false
