# Frontend environment variables for Weekly Report Frontend
# Copy this file to .env and fill in values. Do NOT commit your real .env.

# Supabase project URL and anon public key
REACT_APP_SUPABASE_URL=
REACT_APP_SUPABASE_KEY=

# Base URL where this frontend is served (used for OAuth redirectTo)
# If left empty, the app will fall back to window.location.origin at runtime.
REACT_APP_FRONTEND_URL=

# Backend base URL (REST API). Prefer REACT_APP_API_BASE; REACT_APP_BACKEND_URL is a fallback alias.
REACT_APP_API_BASE=
REACT_APP_BACKEND_URL=

# WebSocket URL (optional)
REACT_APP_WS_URL=

# Build/runtime settings
REACT_APP_NODE_ENV=development
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=debug

# Healthcheck path
REACT_APP_HEALTHCHECK_PATH=/healthz

# Feature flags
# Set to 'true' to disable auth for local testing and allow anonymous access to routes.
REACT_APP_DISABLE_AUTH=false
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=false

# NOTE:
# After setting REACT_APP_FRONTEND_URL, add the following Redirect URLs in Supabase Dashboard > Authentication > URL Configuration:
#   - {REACT_APP_FRONTEND_URL}/auth/callback
#   - {REACT_APP_FRONTEND_URL}
# Also add any preview URLs used by your environment (e.g., Vercel/Netlify preview domains) with the same paths.
